      "dart": {
        "lib/firebase_options.dart": {
          "projectId": "attendo-312ea",
          "configurations": {
            "android": "1:414422473305:android:d042f039ee277ba1ed2f48",
            "ios": "1:414422473305:ios:b5f531f526b3d184ed2f48",
            "macos": "1:414422473305:ios:b5f531f526b3d184ed2f48",
            "web": "1:414422473305:web:64a053640f5ee796ed2f48",
            "windows": "1:414422473305:web:10642ec000e55ee8ed2f48"
          }
        }
