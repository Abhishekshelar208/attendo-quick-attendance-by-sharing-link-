        }
      },
      "macos": {
        "default": {
          "projectId": "attendo-312ea",
          "appId": "1:414422473305:ios:b5f531f526b3d184ed2f48",
          "uploadDebugSymbols": false,
          "fileOutput": "macos/Runner/GoogleService-Info.plist"
